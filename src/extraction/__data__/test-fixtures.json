				"input": "paths:\n  config: /home/user/.config/app.yaml\n  cache: ./cache/temp\nurls:\n  - https://api.example.com/v1/data\n  - file:///usr/local/bin/app",
				"expected": [
					"/home/user/.config/app.yaml",
					"./cache/temp",
					"https://api.example.com/v1/data",
