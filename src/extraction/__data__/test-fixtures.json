{
	"json": {
		"description": "JSON test fixtures with expected path extractions",
		"fixtures": [
			{
				"name": "simple-paths",
				"input": "{\n  \"config\": \"/etc/app/config.json\",\n  \"log\": \"./logs/app.log\",\n  \"data\": \"C:\\\\data\\\\app.db\"\n}",
				"expected": [
					"/etc/app/config.json",
					"./logs/app.log",
					"C:\\data\\app.db"
				]
			},
			{
				"name": "nested-objects",
				"input": "{\n  \"paths\": {\n    \"config\": \"/home/user/.config/app.json\",\n    \"cache\": \"./cache/temp\"\n  },\n  \"urls\": [\n    \"https://api.example.com/v1/data\",\n    \"file:///usr/local/bin/app\"\n  ]\n}",
				"expected": [
					"/home/user/.config/app.json",
					"./cache/temp",
					"https://api.example.com/v1/data",
					"file:///usr/local/bin/app"
				]
			},
			{
				"name": "mixed-types",
				"input": "{\n  \"version\": \"1.0.0\",\n  \"path\": \"/usr/bin/app\",\n  \"relative\": \"../config/settings.yaml\",\n  \"windows\": \"D:\\\\Program Files\\\\App\\\\config.ini\"\n}",
				"expected": [
					"/usr/bin/app",
					"../config/settings.yaml",
					"D:\\Program Files\\App\\config.ini"
				]
			}
		]
	},
