				]
			}
		]
	},
	"javascript": {
		"description": "JavaScript test fixtures with expected path extractions",
		"fixtures": [
			{
				"name": "string-literals",
				"input": "const configPath = '/etc/app/config.json';\nconst logPath = './logs/app.log';\nconst dataPath = 'C:\\\\data\\\\app.db';",
				"expected": [
					"/etc/app/config.json",
					"./logs/app.log",
					"C:\\data\\app.db"
				]
			},
			{
				"name": "template-literals",
				"input": "const configPath = `/etc/app/${env}/config.json`;\nconst logPath = `./logs/${appName}.log`;",
				"expected": ["/etc/app/${env}/config.json", "./logs/${appName}.log"]
			},
			{
				"name": "object-properties",
				"input": "const paths = {\n  config: '/home/user/.config/app.json',\n  cache: './cache/temp',\n  data: 'C:\\\\data\\\\app.db'\n};",
				"expected": [
					"/home/user/.config/app.json",
					"./cache/temp",
					"C:\\data\\app.db"
				]
			}
		]
	},
